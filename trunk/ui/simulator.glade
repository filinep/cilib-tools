<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk+" version="2.22"/>
  <object class="GtkListStore" id="comboModel">
    <columns>
      <!-- column-name Options -->
      <column type="gchararray"/>
    </columns>
  </object>
  <object class="GtkCheckButton" id="runHeader">
    <property name="label" translatable="yes">Run</property>
    <property name="visible">True</property>
    <property name="can_focus">True</property>
    <property name="receives_default">False</property>
    <property name="use_action_appearance">False</property>
    <property name="xalign">0</property>
    <property name="active">True</property>
    <property name="draw_indicator">True</property>
  </object>
  <object class="GtkBox" id="simulator">
    <property name="visible">True</property>
    <property name="can_focus">False</property>
    <property name="orientation">vertical</property>
    <child>
      <object class="GtkToolbar" id="toolbar">
        <property name="visible">True</property>
        <property name="can_focus">False</property>
        <child>
          <object class="GtkToolButton" id="addSimulation">
            <property name="visible">True</property>
            <property name="can_focus">False</property>
            <property name="use_action_appearance">False</property>
            <property name="label" translatable="yes">Add</property>
            <property name="use_underline">True</property>
            <property name="stock_id">gtk-add</property>
            <signal name="clicked" handler="on_add_click" swapped="no"/>
          </object>
          <packing>
            <property name="expand">False</property>
            <property name="homogeneous">True</property>
          </packing>
        </child>
        <child>
          <object class="GtkToolButton" id="runSelected">
            <property name="visible">True</property>
            <property name="can_focus">False</property>
            <property name="use_action_appearance">False</property>
            <property name="label" translatable="yes">Run</property>
            <property name="use_underline">True</property>
            <property name="stock_id">gtk-media-play</property>
            <signal name="clicked" handler="on_run_click" swapped="no"/>
          </object>
          <packing>
            <property name="expand">False</property>
            <property name="homogeneous">True</property>
          </packing>
        </child>
      </object>
      <packing>
        <property name="expand">False</property>
        <property name="fill">True</property>
        <property name="position">0</property>
      </packing>
    </child>
    <child>
      <object class="GtkPaned" id="splitPane">
        <property name="visible">True</property>
        <property name="can_focus">True</property>
        <property name="orientation">vertical</property>
        <property name="position">250</property>
        <property name="position_set">True</property>
        <child>
          <object class="GtkScrolledWindow" id="scrolled">
            <property name="visible">True</property>
            <property name="can_focus">True</property>
            <property name="shadow_type">in</property>
            <child>
              <object class="GtkTreeView" id="treeview">
                <property name="visible">True</property>
                <property name="can_focus">True</property>
                <property name="model">store</property>
                <property name="headers_clickable">False</property>
                <property name="search_column">0</property>
                <child>
                  <object class="GtkTreeViewColumn" id="runColumn">
                    <property name="sizing">autosize</property>
                    <property name="title" translatable="yes">Run</property>
                    <property name="clickable">True</property>
                    <property name="widget">runHeader</property>
                    <signal name="clicked" handler="on_header_toggle" swapped="no"/>
                    <child>
                      <object class="GtkCellRendererToggle" id="cellrenderertoggle1">
                        <signal name="toggled" handler="on_run_toggle" swapped="no"/>
                      </object>
                      <attributes>
                        <attribute name="active">5</attribute>
                      </attributes>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="GtkTreeViewColumn" id="algorithmColumn">
                    <property name="sizing">autosize</property>
                    <property name="title" translatable="yes">Algorithm</property>
                    <child>
                      <object class="GtkCellRendererCombo" id="cellrenderertext1">
                        <property name="editable">True</property>
                        <property name="model">comboModel</property>
                        <property name="text_column">0</property>
                        <signal name="changed" handler="combo_edited" object="algorithmColumn" swapped="no"/>
                        <signal name="editing-started" handler="populate_combo" object="algorithmColumn" swapped="no"/>
                        <signal name="edited" handler="combo_edited" object="algorithmColumn" swapped="no"/>
                      </object>
                      <attributes>
                        <attribute name="text">0</attribute>
                      </attributes>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="GtkTreeViewColumn" id="problemColumn">
                    <property name="sizing">autosize</property>
                    <property name="title" translatable="yes">Problem</property>
                    <child>
                      <object class="GtkCellRendererCombo" id="cellrenderertext2">
                        <property name="editable">True</property>
                        <property name="model">comboModel</property>
                        <property name="text_column">0</property>
                        <signal name="changed" handler="combo_edited" object="problemColumn" swapped="no"/>
                        <signal name="editing-started" handler="populate_combo" object="problemColumn" swapped="no"/>
                        <signal name="edited" handler="combo_edited" object="problemColumn" swapped="no"/>
                      </object>
                      <attributes>
                        <attribute name="text">1</attribute>
                      </attributes>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="GtkTreeViewColumn" id="measurementsColumn">
                    <property name="sizing">autosize</property>
                    <property name="title" translatable="yes">Measurement</property>
                    <child>
                      <object class="GtkCellRendererCombo" id="cellrenderertext3">
                        <property name="editable">True</property>
                        <property name="model">comboModel</property>
                        <property name="text_column">0</property>
                        <signal name="changed" handler="combo_edited" object="measurementsColumn" swapped="no"/>
                        <signal name="editing-started" handler="populate_combo" object="measurementsColumn" swapped="no"/>
                        <signal name="edited" handler="combo_edited" object="measurementsColumn" swapped="no"/>
                      </object>
                      <attributes>
                        <attribute name="text">2</attribute>
                      </attributes>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="GtkTreeViewColumn" id="samplesColumn">
                    <property name="sizing">autosize</property>
                    <property name="title" translatable="yes">Samples</property>
                    <child>
                      <object class="GtkCellRendererText" id="cellrenderertext4">
                        <property name="editable">True</property>
                        <signal name="edited" handler="entry_edited" object="samplesColumn" swapped="no"/>
                      </object>
                      <attributes>
                        <attribute name="text">3</attribute>
                      </attributes>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="GtkTreeViewColumn" id="fileColumn">
                    <property name="sizing">autosize</property>
                    <property name="title" translatable="yes">File</property>
                    <child>
                      <object class="GtkCellRendererText" id="cellrenderertext5">
                        <property name="editable">True</property>
                        <signal name="edited" handler="entry_edited" object="fileColumn" swapped="no"/>
                      </object>
                      <attributes>
                        <attribute name="text">4</attribute>
                      </attributes>
                    </child>
                  </object>
                </child>
              </object>
            </child>
          </object>
          <packing>
            <property name="resize">False</property>
            <property name="shrink">True</property>
          </packing>
        </child>
        <child>
          <object class="GtkScrolledWindow" id="vteScroll">
            <property name="visible">True</property>
            <property name="can_focus">True</property>
            <property name="shadow_type">in</property>
            <child>
              <placeholder/>
            </child>
          </object>
          <packing>
            <property name="resize">True</property>
            <property name="shrink">True</property>
          </packing>
        </child>
      </object>
      <packing>
        <property name="expand">True</property>
        <property name="fill">True</property>
        <property name="position">1</property>
      </packing>
    </child>
  </object>
  <object class="GtkListStore" id="store">
    <columns>
      <!-- column-name Algorithm -->
      <column type="gchararray"/>
      <!-- column-name Problem -->
      <column type="gchararray"/>
      <!-- column-name Measurement -->
      <column type="gchararray"/>
      <!-- column-name Samples -->
      <column type="gchararray"/>
      <!-- column-name File -->
      <column type="gchararray"/>
      <!-- column-name Run -->
      <column type="gboolean"/>
      <!-- column-name MeasureOptions -->
      <column type="GtkListStore"/>
      <!-- column-name ProbOptions -->
      <column type="GtkListStore"/>
      <!-- column-name AlgOptions -->
      <column type="GtkListStore"/>
    </columns>
  </object>
</interface>
